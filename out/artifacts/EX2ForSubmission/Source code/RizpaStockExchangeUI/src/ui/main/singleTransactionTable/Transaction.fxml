<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>

<ScrollPane fitToHeight="true" fitToWidth="true" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ui.main.singleTransactionTable.singleTransactionTableController">
   <content>
      <TableView fx:id="SingleStockTransactionsMadeTableView">
        <columns>
          <TableColumn fx:id="dateCol" prefWidth="85.0" resizable="false" text="Date" />
          <TableColumn fx:id="kindCol" prefWidth="75.0" resizable="false" text="Kind" />
            <TableColumn fx:id="amountCol" prefWidth="75.0" resizable="false" text="Amount" />
            <TableColumn fx:id="priceCol" prefWidth="75.0" resizable="false" text="Price" />
            <TableColumn fx:id="BuyerCol" prefWidth="75.0" resizable="false" text="Buyer" />
            <TableColumn fx:id="SelletrCol" prefWidth="75.0" resizable="false" text="Seller" />
            <TableColumn fx:id="initiatorCol" prefWidth="75.0" resizable="false" text="Initiator" />
        </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
   </content>
</ScrollPane>
